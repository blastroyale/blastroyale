    bottom: 219px;
    left: 15px;
    padding-left: 30px;
    padding-top: 4px;
    padding-bottom: 4px;
    -unity-background-scale-mode: scale-and-crop;
}

#GameModeButton .mutator-icon {
    width: 30px;
    height: 30px;
}

#GameModeButton .speedup-mutator .mutator-icon {
    background-image: url('project://database/Assets/Art/UI/Sprites/GameModeSelection/SpeedUpMutatorIcon.png?fileID=21300000&guid=eab910956b2584d8aa5adae86fc70675&type=3#SpeedUpMutatorIcon');
}

#GameModeButton .quickspecials-mutator .mutator-icon {
    background-image: url('project://database/Assets/Art/UI/Sprites/GameModeSelection/QuickSpecialsMutatorIcon.png?fileID=21300000&guid=1b7e8fb2c20bc42098c49527ddc17ff1&type=3#QuickSpecialsMutatorIcon');
}

#GameModeButton .healthyair-mutator .mutator-icon {
    background-image: url('project://database/Assets/Art/UI/Sprites/GameModeSelection/HealthyAirMutatorIcon.png?fileID=21300000&guid=40a49e478e2a64d8ba5857e4e7116e77&type=3#HealthyAirMutatorIcon');
}

#GameModeButton .mutator-title {
    color: rgb(255, 255, 255);
    font-size: 27px;
    margin-left: 10px;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
}

#GameModeButton #TopImage {
    position: absolute;
}

#GameModeButton #Crown {
    position: absolute;
    left: 50%;
    right: 50%;
    width: 86px;
    height: 58px;
    top: -70px;
}

#GameModeButton.ranked #Crown {
    background-image: url('project://database/Assets/Art/UI/Sprites/GameModeSelection/RankedCrown.png?fileID=2800000&guid=a3323e773a3d94bfda84717a55d45241&type=3#RankedCrown');
}

#GameModeButton #ModeTag {
    width: 130px;
    height: 40px;
    right: -5px;
    top: -12px;
}

#GameModeButton.ranked #ModeTag {
    background-image: url('project://database/Assets/Art/UI/Sprites/GameModeSelection/Rankedtag.svg?fileID=7388822144124034973&guid=ca4852c7187dd4f74905faaea57ea057&type=3#Rankedtag');
}

#GameModeButton.casual #ModeTag {
    background-image: url('project://database/Assets/Art/UI/Sprites/GameModeSelection/CasualTag.svg?fileID=7388822144124034973&guid=570f2f4b2fee6499a9ed88a5cc6bce97&type=3#CasualTag');
}

#GameModeButton #ModeTagTitle {
    color: rgb(255, 255, 255);
    font-size: 30px;
    -unity-text-outline-width: 2px;
}

#GameModeButton.ranked #ModeTagTitle {
    -unity-text-outline-color: rgb(177, 46, 0);
}

#GameModeButton.casual #ModeTagTitle {
    -unity-text-outline-color: rgb(0, 74, 158);
}

.hidden .hidden-gamemodebutton {
    opacity: 0;
    translate: 0 400px;
}

.visible-gamemodebutton1 {
    opacity: 1;
    transition-duration: 0.7s, 0.7s;
    transition-property: opacity, translate;
    transition-delay: 0.7s, 0.7s;
    transition-timing-function: ease, ease-out-back;
}

.visible-gamemodebutton2 {
    opacity: 1;
    transition-duration: 0.7s, 0.7s;
    transition-property: opacity, translate;
    transition-delay: 0.9s, 0.9s;
    transition-timing-function: ease, ease-out-back;
}

.visible-gamemodebutton3 {
    opacity: 1;
    transition-duration: 0.7s, 0.7s;
    transition-property: opacity, translate;
    transition-delay: 1.1s, 1.1s;
    transition-timing-function: ease, ease-out-back;
}

.visible-gamemodebutton4 {
    opacity: 1;
    transition-duration: 0.7s, 0.7s;
    transition-property: opacity, translate;
    transition-delay: 1.3s, 1.3s;
    transition-timing-function: ease, ease-out-back;
}

.visible-gamemodebutton {
    opacity: 1;
}

.hidden .hidden-crown {
    opacity: 0;
    translate: 0 -100px;
}

.visible-crown {
    opacity: 1;
    transition-duration: 0.7s, 0.7s;
    transition-property: opacity, translate;
    transition-delay: 1.5s, 1.5s;
    transition-timing-function: ease, ease-out-back;
}
