                <FirstLight.Game.UIElements.JoystickElement name="MovementJoystick" class="joystick--move" />
            </ui:VisualElement>
            <ui:VisualElement name="AimJoystickArea" class="joystick-container">
                <FirstLight.Game.UIElements.JoystickElement name="ShootingJoystick" class="joystick--aim" />
            </ui:VisualElement>
            <FirstLight.Game.UIElements.ImageButton name="MenuButton" class="menu-button">
                <ui:VisualElement name="Icon" class="menu-button__icon" style="height: 40px;" />
            </FirstLight.Game.UIElements.ImageButton>
            <FirstLight.Game.UIElements.SpecialButtonElement name="Special0" class="special-button--special0" />
            <FirstLight.Game.UIElements.SpecialButtonElement name="Special1" class="special-button--special1" />
            <ui:VisualElement name="TopCenterContainer" picking-mode="Ignore" class="top-center-container">
                <ui:VisualElement name="MatchStatus" class="match-status">
                    <ui:VisualElement name="AliveIcon" class="sprite-match__icon-players-alive" />
                    <ui:Label text="12" name="AliveCountText" class="match-status__playercount" />
                    <ui:VisualElement name="Counter" class="match-status__counter">
