                    <ui:VisualElement name="Pattern" class="dark-pattern__pattern" />
                    <ui:VisualElement name="Pattern" class="dark-pattern__pattern" />
                    <ui:VisualElement name="Pattern" class="dark-pattern__pattern" />
                    <ui:VisualElement name="Pattern" class="dark-pattern__pattern" />
                </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement name="DarkPatternRight" class="dark-pattern dark-pattern--right">
                <ui:VisualElement name="BlackSquare" class="dark-pattern__black-square" />
                <ui:VisualElement name="PatternHolder" class="dark-pattern__pattern-holder">
                    <ui:VisualElement name="Pattern" class="dark-pattern__pattern" />
                    <ui:VisualElement name="Pattern" class="dark-pattern__pattern" />
                    <ui:VisualElement name="Pattern" class="dark-pattern__pattern" />
                    <ui:VisualElement name="Pattern" class="dark-pattern__pattern" />
                    <ui:VisualElement name="Pattern" class="dark-pattern__pattern" />
                    <ui:VisualElement name="Pattern" class="dark-pattern__pattern" />
                    <ui:VisualElement name="Pattern" class="dark-pattern__pattern" />
                </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement name="EquipmentIcon" class="rewards-equipment__item-icon">
                <ui:VisualElement name="SparkGradient1" class="rewards-one__spark-small rewards-one__spark-small--1">
                    <ui:VisualElement name="Spark" class="rewards-one__spark" />
                </ui:VisualElement>
                <ui:VisualElement name="SparkGradient2" class="rewards-one__spark-small rewards-one__spark-small--2">
                    <ui:VisualElement name="Spark" class="rewards-one__spark" />
                </ui:VisualElement>
                <ui:VisualElement name="SparkGradient3" class="rewards-one__spark-small rewards-one__spark-small--3">
                    <ui:VisualElement name="Spark" class="rewards-one__spark" />
                </ui:VisualElement>
                <ui:VisualElement name="AppearGradient" class="rewards-one__spark-gradient">
                    <ui:VisualElement name="AppearSparkEquipment" class="rewards-one__appear-spark" />
                </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement name="EquipmentInfoContainer" class="rewards-equipment__info-container">
                <ui:VisualElement name="NameContainer" class="rewards-equipment__name-container">
                    <ui:Label tabindex="-1" text="GRENADE LAUNCHER" display-tooltip-when-elided="true" name="EquipmentName" class="rewards-equipment__name" />
                    <FirstLight.Game.UIElements.LocalizedLabel tabindex="-1" localization-key="UITRewards/new" text="NEW" display-tooltip-when-elided="true" name="NewLabel" class="rewards-equipment__new-label">
                        <ui:VisualElement name="NewLabelGradient" class="rewards-equipment__new-label-gradient" />
                        <ui:VisualElement name="NewLabelSpark" class="rewards-equipment__new-label-spark" />
                    </FirstLight.Game.UIElements.LocalizedLabel>
                </ui:VisualElement>
                <ui:VisualElement name="TagsContainer" class="rewards-equipment__tags-container">
                    <ui:Label tabindex="-1" text="UNCOMMON" display-tooltip-when-elided="true" name="EquipmentRarityLabel" class="rewards-equipment__tag-label" />
                    <FirstLight.Game.UIElements.LocalizedLabel tabindex="-1" text="LONG RANGE" display-tooltip-when-elided="true" name="EquipmentRangeLabel" class="rewards-equipment__tag-label" />
                </ui:VisualElement>
                <FirstLight.Game.UIElements.EquipmentCardElement name="EquipmentRewardCard" class="rewards-equipment__card" />
            </ui:VisualElement>
            <ui:VisualElement name="WhiteBlink" class="screen-blink-white" />
        </ui:VisualElement>
