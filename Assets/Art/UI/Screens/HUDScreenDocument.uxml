                        </ui:VisualElement>
                    </ui:VisualElement>
                    <ui:VisualElement name="WeaponIconShadow" class="weapon-display__weapon-icon weapon-display__weapon-icon--shadow">
                        <ui:VisualElement name="WeaponIcon" class="weapon-display__weapon-icon">
                            <ui:VisualElement name="FactionIcon" class="weapon-display__weapon-faction-icon sprite-equipmentcard__card-faction-dimensional" />
                        </ui:VisualElement>
                    </ui:VisualElement>
                </ui:VisualElement>
                <ui:Label text="67" name="Ammo" class="weapon-display__ammo">
                    <ui:VisualElement name="Icon" class="sprite-match__icon-ammo weapon-display__ammo-icon" />
                </ui:Label>
            </FirstLight.Game.UIElements.ImageButton>
            <ui:VisualElement name="KillFeed" picking-mode="Ignore" class="kill-feed">
                <FirstLight.Game.UIElements.DeathNotificationElement />
                <FirstLight.Game.UIElements.DeathNotificationElement />
            </ui:VisualElement>
            <ui:VisualElement name="SquadMembers" picking-mode="Ignore" class="squad-members-container">
                <FirstLight.Game.UIElements.SquadMemberElement />
                <FirstLight.Game.UIElements.SquadMemberElement />
            </ui:VisualElement>
            <ui:VisualElement name="EquipmentDisplay" picking-mode="Ignore" class="equipment-display">
                <ui:VisualElement name="Character" picking-mode="Ignore" class="equipment-display__character" style="width: 173px; height: 165px;">
                    <ui:VisualElement name="Armor" picking-mode="Ignore" class="equipment-display__gear-item equipment-display__gear-item--armor" />
                    <ui:VisualElement name="Amulet" picking-mode="Ignore" class="equipment-display__gear-item equipment-display__gear-item--amulet" />
                    <ui:VisualElement name="Helmet" picking-mode="Ignore" class="equipment-display__gear-item equipment-display__gear-item--helmet" />
                    <ui:VisualElement name="Shield" picking-mode="Ignore" class="equipment-display__gear-item equipment-display__gear-item--shield" />
                </ui:VisualElement>
                <ui:Label text="0/4" name="Count" class="equipment-display__count" style="display: none;" />
            </ui:VisualElement>
        </FirstLight.Game.UIElements.SafeAreaElement>
        <ui:VisualElement name="StatusNotifications" picking-mode="Ignore" class="fill-parent" style="background-color: rgba(0, 0, 0, 0); align-items: center;">
