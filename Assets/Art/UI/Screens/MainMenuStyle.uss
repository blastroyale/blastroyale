#LeaderboardsButton .button-icon {
    background-image: url('project://database/Assets/Art/UI/Sprites/IconLeaderboards.png?fileID=21300000&guid=3a7d899cc87a648b6ab9f1f3cf7035f2&type=3#IconLeaderboards');
}

#BattlePassHolder {
    transition-duration: var(--in-animation-duration);
    transition-timing-function: var(--in-animation-easing);
    transition-property: margin-left;
    transition-delay: 0s;
}

#SettingsButton {
    transition-duration: var(--in-animation-duration);
    transition-timing-function: var(--in-animation-easing);
    transition-property: margin-top;
    transition-delay: 0s;
}

#CSCurrency {
