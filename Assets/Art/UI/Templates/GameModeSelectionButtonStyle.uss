#GameModeButton #GameModeDescription {
    color: rgb(0, 0, 0);
    font-size: 30px;
    -unity-text-align: upper-left;
    padding-left: 40px;
    white-space: normal;
}

#GameModeButton.ranked #GameModeDescription {
    color: rgb(120, 3, 23);
}

#GameModeButton.casual #GameModeDescription {
    color: rgb(16, 59, 137);
}

#GameModeButton #GameModeTimer {
    color: rgb(0, 0, 0);
    font-size: 30px;
    -unity-text-align: upper-right;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    position: absolute;
    bottom: 5px;
    right: 30px;
}

#GameModeButton.ranked #GameModeTimer {
    color: rgb(155, 47, 38);
}

#GameModeButton.casual #GameModeTimer {
    color: rgb(16, 59, 137);
}

