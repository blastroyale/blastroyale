#GameModeButton {
    width: 529px;
    height: 763px;
    margin-left: 30px;
    margin-right: 0;
    margin-top: 120px;
    margin-bottom: 100px;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    background-color: rgba(76, 76, 76, 0);
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
}

#GameModeButton.selected #Scalable {
    scale: 1.1 1.1;
    transition-property: scale;
    transition-duration: 0.5s;
    transition-timing-function: ease;
    transition-delay: 0s;
}

#GameModeButton #GameModeOutline {
    -unity-background-image-tint-color: rgba(1, 0, 0, 0);
}

#GameModeButton.selected #GameModeOutline {
    -unity-background-image-tint-color: rgb(255, 255, 255);
}

#GameModeButton #GameModeIcon {
    width: 50px;
    height: 50px;
    margin-left: 40px;
}

#GameModeButton.battleroyale #GameModeIcon {
    background-image: url('project://database/Assets/Art/UI/Sprites/GameModeSelection/BattleRoyaleModeIcon.png?fileID=2800000&guid=c8082fa22a90a4b92b5cd7966cf32cc6&type=3#BattleRoyaleModeIcon');
}

#GameModeButton.deathmatch #GameModeIcon {
    background-image: url('project://database/Assets/Art/UI/Sprites/GameModeSelection/DeathmatchModeIcon.png?fileID=2800000&guid=b1b4a0f0d286243dca54199c42a09b34&type=3#DeathmatchModeIcon');
}

#GameModeButton.custom #GameModeIcon {
    background-image: url('project://database/Assets/Art/UI/Sprites/GameModeSelection/CustomgameModeIcon.png?fileID=2800000&guid=4dcea76e0c52e43d6b931b8c957efe36&type=3#CustomgameModeIcon');
}

#GameModeButton #DataPanel {
    width: 404px;
    height: 189px;
