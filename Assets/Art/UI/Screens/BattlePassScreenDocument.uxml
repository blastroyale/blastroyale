        <ui:VisualElement name="reference" style="left: 0; position: absolute; background-image: url(&apos;project://database/Assets/Art/UI/References/Battle%20Pass.png?fileID=2800000&amp;guid=40ad9298235317e47978e24927712d61&amp;type=3#Battle Pass&apos;); top: 0; right: 0; bottom: 0; width: 100%; height: 100%; -unity-background-image-tint-color: rgba(255, 255, 255, 0);" />
    </ui:VisualElement>
