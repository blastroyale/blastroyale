                <ui:VisualElement name="SparkGradient3" class="rewards-one__spark-small rewards-one__spark-small--3">
                    <ui:VisualElement name="Spark" class="rewards-one__spark" />
                </ui:VisualElement>
                <ui:VisualElement name="AppearGradient" class="rewards-one__spark-gradient">
                    <ui:VisualElement name="AppearSparkEquipment" class="rewards-one__appear-spark" />
                </ui:VisualElement>
            </ui:VisualElement>
        </ui:VisualElement>
        <ui:VisualElement name="EquipmentReward" class="rewards-equipment" style="display: none;">
            <ui:VisualElement name="RaysEquipment" class="rewards-equipment__rays">
                <ui:VisualElement name="RaysStrongerGradient" class="rewards-equipment__gradient-stronger" />
