:root {
    --out-animation-duration: 0.3s;
    --out-animation-easing: ease-in;
    --in-animation-duration: 0.3s;
    --in-animation-easing: ease-out;
    --animation-delay-1: 0s;
    --animation-delay-2: 0.1s;
    --animation-delay-3: 0.2s;
    --animation-delay-4: 0.3s;
    --animation-delay-5: 0.4s;
    --animation-total-duration: 0.7s;
}

#PlayButton {
    background-image: url('project://database/Assets/Art/UI/Sprites/HomeScreen/PlayButton.png?fileID=21300000&guid=7b2a8cf62857f439d94b28d33014167f&type=3#PlayButton');
    width: 535px;
    height: 193px;
    font-size: 70px;
    color: rgb(255, 255, 255);
    -unity-font-style: bold;
    -unity-text-outline-width: 2px;
    -unity-text-outline-color: rgb(141, 27, 0);
    padding-left: 38px;
    text-shadow: 5px 5px 0 rgb(139, 26, 3);
    -unity-text-align: middle-center;
    padding-bottom: 19px;
}

#PlayButton:active {
    background-image: url('project://database/Assets/Art/UI/Sprites/HomeScreen/PlayButtonPressed.png?fileID=21300000&guid=4755d43108e94404e975817fcc3c9d0b&type=3#PlayButtonPressed');
    padding-left: 62px;
    padding-top: 20px;
}

#GameModeButton {
    background-image: url('project://database/Assets/Art/UI/Sprites/HomeScreen/ChangemodeButton.png?fileID=2800000&guid=c9a202b2307fb49549d013e0c91bcb44&type=3#ChangemodeButton');
    width: 414px;
    height: 168px;
    margin-bottom: 7px;
    padding-left: 59px;
    padding-bottom: 4px;
    transition-delay: var(--animation-delay-2);
    transition-timing-function: var(--in-animation-easing);
    transition-property: margin-right;
    transition-duration: var(--in-animation-duration);
}

#CustomGameButton {
    transition-delay: var(--animation-delay-3);
    transition-property: margin-right;
    transition-duration: var(--in-animation-duration);
    transition-timing-function: var(--in-animation-easing);
}

#StoreButton {
    transition-delay: var(--animation-delay-4);
    transition-property: margin-right;
    transition-duration: var(--in-animation-duration);
    transition-timing-function: var(--in-animation-easing);
}

#GameModeButton:active {
    background-image: url('project://database/Assets/Art/UI/Sprites/HomeScreen/ChangemodeButtonPressed.png?fileID=21300000&guid=71761bd5e01a740118d4f5b0de3cb413&type=3#ChangemodeButtonPressed');
    padding-left: 68px;
    padding-top: 45px;
}

#PlayerNameLabel {
    position: absolute;
    top: 90px;
    left: 10px;
    rotate: -15deg;
    -unity-text-align: lower-center;
    width: 300px;
    height: 70px;
    overflow: hidden;
    white-space: normal;
}

#BattlePassButton {
    background-image: url('project://database/Assets/Art/UI/Sprites/HomeScreen/BattlepassButton.png?fileID=21300000&guid=2944494fed89644cabdbd0b3da73c0e3&type=3#BattlepassButton');
    width: 479px;
    height: 179px;
    padding-top: 24px;
}

#BattlePassButton #Title {
    text-shadow: -5px 5px 0 rgb(139, 26, 3);
    font-size: 55px;
    -unity-font-style: bold;
    -unity-text-outline-width: 2px;
    -unity-text-outline-color: rgb(141, 27, 0);
    -unity-text-align: upper-center;
}

#BattlePassButton #BattlePassProgressElement {
    background-image: url('project://database/Assets/Art/UI/Sprites/HomeScreen/BattlePassProgress.png?fileID=21300000&guid=805b077c9a6dc4cc8acf308e183b7a0b&type=3#BattlePassProgress');
}

#BattlePassButton #BattlePassCrownIcon {
    position: absolute;
    top: -27px;
    right: 5px;
}

#BattlePassButton:active {
    background-image: url('project://database/Assets/Art/UI/Sprites/HomeScreen/BattlepassButtonPressed.png?fileID=21300000&guid=3954eea7714304c3aacf010642a02d78&type=3#BattlepassButtonPressed');
    padding-top: 30px;
    padding-right: 21px;
    padding-left: 0;
}

#BattlePassButton:active #BattlePassCrownIcon {
    position: absolute;
    top: -20px;
    right: 13px;
}

#ChestButton {
}

#GameModeButton #GameTypeLabel {
    color: rgb(146, 137, 162);
}

#EquipmentButton {
    transition-duration: var(--in-animation-duration);
    transition-timing-function: var(--in-animation-easing);
    transition-property: margin-left;
    transition-delay: var(--animation-delay-4);
}

#HeroesButton {
    transition-duration: var(--in-animation-duration);
    transition-timing-function: var(--in-animation-easing);
    transition-property: margin-left;
    transition-delay: var(--animation-delay-3);
}

#LeaderboardsButton {
    transition-duration: var(--in-animation-duration);
    transition-timing-function: var(--in-animation-easing);
    transition-property: margin-left;
    transition-delay: var(--animation-delay-2);
}

#BattlePassHolder {
    transition-duration: var(--in-animation-duration);
    transition-timing-function: var(--in-animation-easing);
    transition-property: margin-left;
    transition-delay: var(--animation-delay-1);
}

.button-with-icon--store > .button-with-icon__icon {
    background-image: url('project://database/Assets/Art/UI/Sprites/HomeScreen/IconEquipment.png?fileID=21300000&guid=f074c3dd0ff0849fe90a973fd6fbd458&type=3#IconEquipment');
}

.button-with-icon--equipment > .button-with-icon__icon {
    background-image: url('project://database/Assets/Art/UI/Sprites/HomeScreen/IconEquipment.png?fileID=21300000&guid=f074c3dd0ff0849fe90a973fd6fbd458&type=3#IconEquipment');
}

.button-with-icon--heroes > .button-with-icon__icon {
    background-image: url('project://database/Assets/Art/UI/Sprites/HomeScreen/IconHeroes.png?fileID=21300000&guid=51ac3af9acd824af591121ab7510c3df&type=3#IconHeroes');
}

.button-with-icon--leaderboards > .button-with-icon__icon {
    background-image: url('project://database/Assets/Art/UI/Sprites/HomeScreen/IconLeaderboards.png?fileID=21300000&guid=3a7d899cc87a648b6ab9f1f3cf7035f2&type=3#IconLeaderboards');
}

.button-with-icon--shop > .button-with-icon__icon {
    background-image: url('project://database/Assets/Art/UI/Sprites/HomeScreen/IconShop.png?fileID=21300000&guid=2ea4ca43a1f724d659125eeae517ed2a&type=3#IconShop');
}

.button-with-icon--custom-game > .button-with-icon__icon {
    background-image: url('project://database/Assets/Art/UI/Sprites/HomeScreen/IconCustomGame.png?fileID=21300000&guid=edeb643c81c524ca28070f239ae51a4e&type=3#IconCustomGame');
}

#SettingsButton {
    transition-duration: var(--in-animation-duration);
    transition-timing-function: var(--in-animation-easing);
    transition-property: margin-top;
    transition-delay: var(--animation-delay-1);
}

#SettingsButton:active > VisualElement {
    -unity-background-image-tint-color: rgb(126, 126, 126);
}

#CSCurrency {
    transition-duration: var(--in-animation-duration);
    transition-timing-function: var(--in-animation-easing);
    transition-property: margin-top;
    transition-delay: var(--animation-delay-2);
}

#BLSTCurrency {
    transition-duration: var(--in-animation-duration);
    transition-timing-function: var(--in-animation-easing);
    transition-property: margin-top;
    transition-delay: var(--animation-delay-3);
}

#PlayerHolder {
    transition-duration: var(--in-animation-duration);
    transition-timing-function: var(--in-animation-easing);
    transition-property: margin-top;
    transition-delay: var(--animation-delay-1);
}

.hidden {
    visibility: hidden;
    display: none;
    transition-delay: var(--animation-total-duration);
}

.hidden .hidden-left {
    margin-left: -1000px;
    transition-duration: var(--out-animation-duration);
    transition-timing-function: var(--out-animation-easing);
    transition-property: margin-left;
}

.hidden .hidden-right {
    margin-right: -700px;
    transition-duration: var(--out-animation-duration);
    transition-timing-function: var(--out-animation-easing);
    transition-property: margin-right;
}

.hidden .hidden-top {
    margin-top: -200px;
    transition-duration: var(--out-animation-duration);
    transition-timing-function: var(--out-animation-easing);
    transition-property: margin-top;
}

.hidden .hidden-profile {
    margin-top: -300px;
    transition-duration: var(--out-animation-duration);
    transition-timing-function: var(--out-animation-easing);
    transition-property: margin-top;
}

Button {
    background-color: rgba(0, 0, 0, 0);
    border-width: 0;
}

.pool-container {
    position: absolute;
    background-image: url('project://database/Assets/Art/UI/Sprites/HomeScreen/PoolHeader.png?fileID=21300000&guid=fe2093cbd35914e9e9cd6c56ee79fca3&type=3#PoolHeader');
    width: 428px;
    height: 102px;
    top: -42px;
    left: -9px;
    flex-direction: row;
    justify-content: space-between;
    font-size: 28px;
    color: rgb(141, 132, 155);
    align-items: stretch;
}
