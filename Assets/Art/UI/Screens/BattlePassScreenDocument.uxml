<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" flg="FirstLight.Game.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" MemoryProfiler="Unity.MemoryProfiler.Editor" noNamespaceSchemaLocation="../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/Art/UI/Screens/BattlePassScreenStyle.uss?fileID=7433441132597879392&amp;guid=dc4d7d108099c400695438f2df23550d&amp;type=3#BattlePassScreenStyle" />
    <ui:VisualElement name="root" picking-mode="Ignore" class="fill-parent screen-root">
        <ui:VisualElement name="SeasonContent" class="season-content">
            <ui:ScrollView focusable="true" name="RewardsScroll" mode="Horizontal" touch-scroll-type="Elastic" horizontal-scroller-visibility="Hidden" vertical-scroller-visibility="Hidden" class="season-content__scroll anim-delay-0 anim-translate anim-translate--left-xxxl" />
            <ui:VisualElement name="SeasonImage" class="season-image anim-delay-0 anim-translate anim-translate--right-xl">
                <ui:VisualElement name="Mask" class="season-image__mask">
                    <ui:VisualElement name="Image" class="season-image__bp-image" />
                </ui:VisualElement>
            </ui:VisualElement>
            <FirstLight.Game.UIElements.LocalizedButton text="##" name="ClaimButton" localization-key="UITBattlePass/claim_all" class="sfx-click-forwards claim-button anim-delay-0 anim-translate anim-translate--up-s" />
            <ui:VisualElement name="SeasonInfo" class="season-info anim-delay-0 anim-translate anim-translate--up-s">
                <ui:VisualElement name="SeasonEndInfo" class="season-info__season-end hidden">
                    <FirstLight.Game.UIElements.LocalizedLabel text="##" name="SeasonEndsLabel" localization-key="UITBattlePass/season_ends_in" class="season-info__label" />
                    <ui:Label text=" 10D 10H" display-tooltip-when-elided="true" name="TimeLeftLabel" class="season-info__label season-info__label--yellow" />
                </ui:VisualElement>
                <FirstLight.Game.UIElements.LocalizedLabel text="##" name="GameplayInfo" localization-key="UITBattlePass/play_games_info" class="season-info__label" />
            </ui:VisualElement>
        </ui:VisualElement>
        <ui:VisualElement name="SeasonHeader" class="season-header anim-delay-0 anim-translate anim-translate--down-s">
            <ui:VisualElement name="Background" class="season-header__background" />
            <ui:VisualElement name="Content" class="season-header__content">
                <ui:VisualElement name="CurrentLevel" class="season-level">
                    <ui:Label text="LV" display-tooltip-when-elided="true" name="Label" class="season-level__label" />
                    <ui:Label display-tooltip-when-elided="true" name="CurrentLevelValue" text="0" class="season-level__label season-level__label--big" />
                </ui:VisualElement>
                <ui:VisualElement name="Spacer" class="width-spacer width-spacer--xl" />
                <ui:VisualElement name="BppBar" class="bpp-bar">
                    <ui:VisualElement name="BppBackground" class="bpp-bar__background" />
                    <ui:VisualElement name="BppProgress" class="bpp-bar__progress" />
                    <ui:Label text="0 / 100" display-tooltip-when-elided="true" name="BppProgressLabel" class="bpp-bar__label" />
                    <ui:VisualElement name="Icon" class="bpp-bar__icon" />
                </ui:VisualElement>
                <ui:VisualElement name="Spacer" class="width-spacer" />
                <ui:VisualElement name="NextLevel" class="season-level">
                    <ui:Label text="LV" display-tooltip-when-elided="true" name="Label" class="season-level__label" />
                    <ui:Label text="1" display-tooltip-when-elided="true" name="NextLevelValue" class="season-level__label season-level__label--big" />
                </ui:VisualElement>
                <ui:VisualElement name="Spacer" class="width-spacer width-spacer--xxxl" />
                <FirstLight.Game.UIElements.CurrencyDisplayElement name="CurrencyDisplayElement" />
                <ui:VisualElement name="Spacer" class="width-spacer width-spacer--xl" />
                <FirstLight.Game.UIElements.CurrencyDisplayElement name="CurrencyDisplayElement" currency="COIN" />
            </ui:VisualElement>
        </ui:VisualElement>
        <FirstLight.Game.UIElements.ScreenHeaderElement focusable="false" name="Header" />
    </ui:VisualElement>
