            <ui:VisualElement name="FriendsContainer" style="flex-grow: 1;">
                <FirstLight.Game.UIElements.LabelOutlined tabindex="-1" text="FRIENDS ONLINE (0)" name="FriendsOnline" class="category-title" />
                <ui:ListView name="FriendsOnlineList" virtualization-method="DynamicHeight" class="container-online-friends" />
