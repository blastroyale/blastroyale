            </ui:VisualElement>
            <ui:Button name="TrophiesHolder" class="player-header__trophies-holder">
                <ui:VisualElement name="TrophyIcon" class="player-header__trophy-icon" />
                <ui:Label text="69420" name="TrophiesAmount" class="player-header__trophies_label" />
            </ui:Button>
        </FirstLight.Game.UIElements.ImageButton>
    </ui:VisualElement>
</ui:UXML>
