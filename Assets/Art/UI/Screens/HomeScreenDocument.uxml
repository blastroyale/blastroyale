                            <FirstLight.Game.UIElements.LocalizedLabel localization-key="UITHomeScreen/blast_pass" name="BattlePassTitle" picking-mode="Ignore" class="battle-pass-button__title" />
                            <FirstLight.Game.UIElements.LocalizedLabel localization-key="UITHomeScreen/blast_pass_claim_reward" name="BattlePassTitleClaimReward" picking-mode="Ignore" class="battle-pass-button__title_claim_reward" />
                            <ui:VisualElement name="BattlePassLevelHolder" class="battle-pass-level-holder">
                                <ui:Label text="5" display-tooltip-when-elided="true" name="BattlePassLevelLabel" class="battle-pass-button__level" />
                                <ui:Label text="LV" name="BattlePassLevelTextLabel" picking-mode="Ignore" class="battle-pass-button__level__text battle-pass-button__leveltext" />
                            </ui:VisualElement>
                            <ui:VisualElement name="BattlePassProgressBg" class="battle-pass-button__progress-bg">
                                <ui:VisualElement name="BattlePassProgressElement" class="battle-pass-button__progress" />
                            </ui:VisualElement>
                            <ui:VisualElement name="BattlePassCrownIcon" class="battle-pass-button__crown" />
                        </ui:VisualElement>
                    </FirstLight.Game.UIElements.ImageButton>
                </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement name="PartyContainer" class="squad-container anim-delay-2 anim-translate anim-translate--left-xl">
                <ui:Label text="PARTY [0]" name="PartyLabel" class="squad-container__label" />
                <ui:ListView focusable="true" name="PartyList" show-bound-collection-size="false" selection-type="None" fixed-item-height="50" class="squad-container__list" />
            </ui:VisualElement>
        </FirstLight.Game.UIElements.SafeAreaElement>
        <FirstLight.Game.UIElements.ImageButton name="Header" class="player-header sfx-click-forwards">
            <ui:VisualElement name="ProfileHolder" class="player-header__profile-holder">
