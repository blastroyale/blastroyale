            <ui:VisualElement name="DarkPatternLeft" class="dark-pattern dark-pattern--left">
                <ui:VisualElement name="BlackSquare" class="dark-pattern__black-square" />
                <ui:VisualElement name="PatternHolder" class="dark-pattern__pattern-holder">
