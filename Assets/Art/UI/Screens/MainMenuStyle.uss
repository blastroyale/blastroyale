}

.hidden {
    visibility: hidden;
    display: none;
    transition-delay: 0.7s;
}

.hidden .hidden-left {
    margin-left: -700px;
    transition-duration: var(--out-animation-duration);
    transition-timing-function: var(--out-animation-easing);
    transition-property: margin-left;
}

.hidden .hidden-right {
    margin-right: -700px;
    transition-duration: var(--out-animation-duration);
    transition-timing-function: var(--out-animation-easing);
    transition-property: margin-right;
}

.hidden .hidden-top {
    margin-top: -200px;
    transition-duration: var(--out-animation-duration);
    transition-timing-function: var(--out-animation-easing);
    transition-property: margin-top;
}

.hidden .hidden-profile {
    margin-top: -300px;
    transition-duration: var(--out-animation-duration);
    transition-timing-function: var(--out-animation-easing);
    transition-property: margin-top;
}
