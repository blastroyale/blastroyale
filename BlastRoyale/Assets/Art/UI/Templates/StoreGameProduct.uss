.product-widget {
    flex-grow: 0;
    width: 442px;
    height: 674px;
    background-image: url('project://database/Assets/Art/UI/Sprites/Home/background-big-bundle.png?fileID=21300000&guid=163b6e338259e914a80538a3c1634c86&type=3#background-big-bundle');
    justify-content: flex-end;
    align-items: center;
    align-self: auto;
    padding-top: 2px;
    overflow: visible;
}

.product-name {
    position: absolute;
    font-size: 46px;
    top: auto;
    left: 2px;
    color: rgb(255, 207, 51);
    -unity-text-outline-width: 3px;
    -unity-text-outline-color: rgb(0, 0, 0);
    text-shadow: 3px 10px 0 rgb(0, 0, 0);
    bottom: 109px;
    width: 416px;
    -unity-text-align: upper-center;
    height: auto;
    text-overflow: clip;
    white-space: normal;
    margin: 0;
    padding: 0;
    letter-spacing: -1px;
}

.product-image {
    height: 480px;
    width: 480px;
    position: absolute;
    align-self: center;
}

.product-background {
    width: 410px;
    height: 104px;
    justify-content: flex-end;
    background-color: rgba(231, 0, 0, 0.72);
}

.product-price {
    font-size: 56px;
    color: rgb(255, 255, 255);
    -unity-text-outline-width: 2px;
    -unity-text-outline-color: rgb(0, 0, 0);
    text-shadow: 3px 6px 0 rgb(0, 0, 0);
    align-self: center;
    width: 410px;
    height: 104px;
    background-image: url('project://database/Assets/Art/UI/Sprites/Home/background-price.png?fileID=21300000&guid=29570bb82136e8a488edae12936f5b0f&type=3#background-price');
    left: -10px;
    -unity-text-align: middle-center;
    top: -3px;
}

.widget-effect {
    width: 370px;
    height: 412px;
    background-image: url('project://database/Assets/Art/UI/Sprites/Home/background-dots-effect.png?fileID=21300000&guid=fa8e92fa48999ee4a8da9ee1555b2962&type=3#background-dots-effect');
    -unity-background-image-tint-color: rgba(255, 255, 255, 0.11);
    align-self: center;
    position: absolute;
}

.product-name--small {
    bottom: 56px;
    font-size: 33px;
    -unity-text-outline-width: 2px;
    text-shadow: 2px 4px 0 rgb(0, 0, 0);
    width: 296px;
}

.product-image-gradient-sides {
    position: absolute;
    background-image: url('project://database/Assets/Art/UI/Sprites/Home/background-bundle-image-gradient.png?fileID=21300000&guid=f2508f9cfda92404e868de558956a4bb&type=3#background-bundle-image-gradient');
    width: 426px;
    height: 566px;
    -unity-background-image-tint-color: rgba(27, 57, 161, 0.58);
}

.product-image-gradient-big {
    background-image: url('project://database/Assets/Art/UI/Sprites/Match/gradientwhiteintense.png?fileID=21300000&guid=9703450dfb82ed84cba13dcd159b2bf4&type=3#gradientwhiteintense');
    width: 440px;
    height: 440px;
    -unity-background-image-tint-color: rgb(82, 182, 255);
    position: absolute;
}

.product-background-image {
    width: 426px;
    height: 576px;
    background-image: url('project://database/Assets/Art/UI/Sprites/Home/background-bundle-image.png?fileID=21300000&guid=990907a628c5e684086dd1420f31d792&type=3#background-bundle-image');
    -unity-background-image-tint-color: rgb(63, 115, 216);
    align-items: center;
    justify-content: center;
}

.product-widget--small {
    width: 312px;
    height: 312px;
    background-image: url('project://database/Assets/Art/UI/Sprites/Home/background-small-bundle.png?fileID=21300000&guid=a9cca8098829b3644beb246b0ca28c57&type=3#background-small-bundle');
    margin-bottom: 30px;
}

.product-image-gradient-small {
    background-image: url('project://database/Assets/Art/UI/Sprites/Match/gradientwhiteintense.png?fileID=21300000&guid=9703450dfb82ed84cba13dcd159b2bf4&type=3#gradientwhiteintense');
    width: 160px;
    height: 160px;
    -unity-background-image-tint-color: rgb(253, 255, 164);
    position: absolute;
}

.product-image--small {
    width: 230px;
    height: 230px;
}

.product-price--small {
    width: 280px;
    height: 54px;
    background-image: url('project://database/Assets/Art/UI/Sprites/Home/background-price-small.png?fileID=21300000&guid=37153d4e2163e1a4ca45768ec9e20693&type=3#background-price-small');
    font-size: 37px;
    -unity-text-outline-width: 1px;
    text-shadow: 2px 4px 0 rgb(0, 0, 0);
    -unity-text-outline-color: rgb(0, 0, 0);
}

.product-image-gradient-big--small {
    width: 374px;
    height: 310px;
}

.product-image-gradient-sides--small {
    position: absolute;
    height: 258px;
    width: 300px;
}

.widget-effect--small {
    width: 260px;
    height: 260px;
}

.product-background-image--small {
    background-image: url('project://database/Assets/Art/UI/Sprites/Home/background-bundle-image-small.png?fileID=21300000&guid=0c49a0f4b2daf7a439b199f361ff0f95&type=3#background-bundle-image-small');
    width: 301px;
    height: 290px;
}

.product-background-image--purple {
    -unity-background-image-tint-color: rgb(182, 51, 228);
}

.product-image-gradient-sides--purple {
    -unity-background-image-tint-color: rgb(87, 32, 157);
}

.product-image-gradient-big--purple {
    -unity-background-image-tint-color: rgb(182, 51, 228);
}

.product-image-gradient-small--purple {
    -unity-background-image-tint-color: rgb(252, 172, 255);
}

.widget-effect--purple {
    -unity-background-image-tint-color: rgba(255, 126, 247, 0.42);
}

.image-tint--yellow {
    color: rgb(255, 192, 0);
}

.product-background-image--yellow {
    -unity-background-image-tint-color: rgb(255, 140, 0);
}

.product-image-gradient-sides--yellow {
    -unity-background-image-tint-color: rgb(190, 74, 58);
}

.product-image-gradient-big--yellow {
    -unity-background-image-tint-color: rgb(255, 188, 58);
}

.product-image-gradient-small--yellow {
    -unity-background-image-tint-color: rgb(250, 231, 130);
}

.widget-effect--yellow {
    -unity-background-image-tint-color: rgba(254, 216, 101, 0.68);
}

.image-tint--green {
    color: rgb(67, 176, 25);
}

.product-background-image--green {
    -unity-background-image-tint-color: rgb(67, 176, 25);
}

.product-image-gradient-sides--green {
    -unity-background-image-tint-color: rgb(1, 131, 45);
}

.product-image-gradient-big--green {
    -unity-background-image-tint-color: rgb(186, 255, 0);
}

.product-image-gradient-small--green {
    -unity-background-image-tint-color: rgb(221, 255, 164);
}

.widget-effect--green {
    -unity-background-image-tint-color: rgba(221, 255, 164, 0.35);
}

.info-click-area {
    flex-grow: 0;
    display: flex;
    visibility: visible;
    width: 90px;
    height: 90px;
    position: absolute;
    top: 0;
    right: 0;
    translate: 25% -25%;
    align-items: center;
    justify-content: center;
    flex-shrink: 0;
}

.info-button {
    background-image: var(--sprite-home__button-info);
    width: 90%;
    height: 90%;
    flex-shrink: 0;
    justify-content: center;
    align-items: center;
}

.owned-stamp {
    flex-grow: 1;
    width: 168px;
    height: 57px;
    position: absolute;
    background-image: none;
    -unity-background-image-tint-color: rgb(255, 255, 255);
    justify-content: center;
    align-self: center;
    top: auto;
    left: 128px;
    text-shadow: 3px 3px 0 rgb(0, 0, 0);
    -unity-text-outline-width: 1px;
    border-top-width: 1px;
    border-right-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    display: flex;
    bottom: 20px;
}

.owned-stamp__bg {
    flex-grow: 1;
    width: 169px;
    height: 57px;
    position: absolute;
    background-image: url('project://database/Assets/Art/UI/Sprites/Home/button-bpreward-outline.png?fileID=2800000&guid=57e23c21c38ad444b8edc6fd8e9eac32&type=3#button-bpreward-outline');
    -unity-background-image-tint-color: rgb(255, 0, 0);
    justify-content: center;
    align-self: center;
    display: flex;
    -unity-background-scale-mode: stretch-to-fill;
}

.owned-stamp__text {
    align-self: center;
    justify-content: center;
    -unity-text-align: middle-center;
    font-size: 49px;
    color: rgb(255, 255, 255);
    -unity-text-outline-color: rgb(0, 0, 0);
    width: 366px;
    flex-grow: 1;
    flex-shrink: 0;
    align-items: center;
}

.info-button--owned {
    background-image: var(--sprite-home__icon-checkmark);
    -unity-background-scale-mode: scale-to-fit;
}

.product-name--owned {
    color: rgb(183, 174, 106);
}

.product-image--owned {
    -unity-background-image-tint-color: rgb(90, 82, 106);
}

.owned-overlay {
    flex-grow: 1;
    position: absolute;
    width: 100%;
    height: 100%;
    background-image: var(--sprite-home__background-bundle-image);
    top: 2px;
    left: 2px;
    -unity-background-image-tint-color: rgba(49, 45, 71, 0.45);
    background-color: rgba(255, 255, 255, 0);
    display: none;
}
