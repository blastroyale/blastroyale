<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="False">
    <Style src="project://database/Assets/Art/UI/Screens/HUDScreenStyle.uss?fileID=7433441132597879392&amp;guid=9db6e0e9477634e1699c3ae2172101f4&amp;type=3#HUDScreenStyle" />
    <ui:VisualElement name="root" class="fill-parent">
        <ui:VisualElement name="PlayerBars" picking-mode="Ignore" usage-hints="GroupTransform" class="fill-parent">
            <FirstLight.Game.UIElements.HealthStatusBarElement name="HealthStatusBarElement" style="position: absolute; left: 1107px; top: 246px;" />
            <FirstLight.Game.UIElements.PlayerStatusBarElement style="left: 1065px; top: 313px;">
                <ui:VisualElement name="AbovePlayerNotifications" class="max-hp-shield-ammo__container" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0);">
                    <ui:Label tabindex="-1" text="MAX" display-tooltip-when-elided="true" name="MaxShieldLabel" class="max-shield__label" style="width: 94px; height: 37px;">
                        <ui:VisualElement name="MaxShieldIcon" class="max-shield__icon" />
                    </ui:Label>
                    <ui:Label tabindex="-1" text="MAX" display-tooltip-when-elided="true" name="MaxHPLabel" class="max-hp__label">
                        <ui:VisualElement name="MaxHPIcon" class="max-hp__icon" />
                    </ui:Label>
                    <ui:Label tabindex="-1" text="MAX" display-tooltip-when-elided="true" name="MaxAmmoLabel" class="max-ammo__label">
                        <ui:VisualElement name="MaxAmmoIcon" class="max-ammo__icon" />
                    </ui:Label>
                    <ui:Label tabindex="-1" text="+1 Level" display-tooltip-when-elided="true" name="PlusLevelLabel" class="plus-level__label" />
                </ui:VisualElement>
            </FirstLight.Game.UIElements.PlayerStatusBarElement>
        </ui:VisualElement>
        <FirstLight.Game.UIElements.SafeAreaElement class="safe-area">
