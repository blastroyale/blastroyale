                        <ui:VisualElement name="BG" class="match-status__bg" />
                        <ui:Label text="30" name="TimerText" class="match-status__counter-text" />
                    </ui:VisualElement>
                    <ui:Label text="12" name="KilledCountText" class="match-status__playercount" />
                    <ui:VisualElement name="KilledIcon" class="sprite-match__icon-players-killed" />
                    <ui:Label text="AREA SHRINKING" name="NotificationText" class="status-areashrinking" />
                </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement name="DeviceStatus" picking-mode="Ignore" class="device-status-bar">
                <ui:VisualElement name="BatteryIcon" class="icon-battery sprite-match__icon-battery-medium" />
                <ui:VisualElement name="WifiIcon" class="icon-wifi sprite-match__icon-wifi-medium" />
                <ui:Label text="49 ms" name="LatencyLabel" class="latency-label" />
            </ui:VisualElement>
