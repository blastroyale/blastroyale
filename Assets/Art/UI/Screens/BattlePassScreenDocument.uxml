                <ui:VisualElement name="Spacer" class="width-spacer" />
                <ui:VisualElement name="NextLevel" class="season-level">
                    <ui:Label text="LV" display-tooltip-when-elided="true" name="Label" class="season-level__label" />
                    <ui:Label text="1" display-tooltip-when-elided="true" name="Value" class="season-level__label season-level__label--big" />
                </ui:VisualElement>
                <ui:VisualElement name="Spacer" class="width-spacer width-spacer--xxxl" />
